"Load default settings from vimrc_example
source /etc/vim/vimrc

"Use plug as package manager
source ~/vim_config/plug.vim

"Load all plugs
source ~/vim_config/vim_plugs.vim

"My Default vim settings
source ~/vim_config/vim_general.vim

"Default vim airline settings
source ~/vim_config/vim_airLine.vim

"C sharp related settings
source ~/vim_config/vim_Csharp.vim
